    "eslint-plugin-react-hooks": "^4.6.0",
    "rc-upload": "^4.3.4",
    "rc-util": "^5.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "rehype": "~12.0.1",
    "rehype-prism-plus": "1.5.0",
    "styled-components": "^5.3.6",
    "vite-plugin-qiankun": "^1.0.15"
