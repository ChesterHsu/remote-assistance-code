    "electron": "^22.0.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "qiankun": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "sass": "^1.57.1",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-electron-plugin": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-require-transform": "^1.0.4",
    "vite-plugin-svgr": "^2.4.0"
